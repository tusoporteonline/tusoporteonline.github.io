	padding-left: 15px;
}
.featured-products .doubleline-2 {
	background-image: url(../images/lines/double-line-2.jpg);
	background-repeat: no-repeat;
	height: 4px;
	width: 100px;
	margin-top: 32px;
	margin-bottom: 20px;
	background-position: 15px;
}
.featured-products .description {
	font-size: 15px;
